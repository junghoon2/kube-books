---
kind: RuntimeClass
apiVersion: node.k8s.io/v1{{ 'beta1' if kube_version is version('v1.20.0', '<') else '' }}
metadata:
  name: kata-qemu
handler: kata-qemu
{% if kata_containers_qemu_overhead %}
overhead:
  podFixed:
    cpu: {{ kata_containers_qemu_overhead_fixed_cpu }}
    memory: {{ kata_containers_qemu_overhead_fixed_memory }}
{% endif %}
